{
  "name": "city-portals-network/mongo-phinx",
  "description": "MongoDB Phinx Adapter",
  "keywords": [
    "MongoDB",
    "phinx"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Garipov Azamat",
      "email": "a.garipov@info74.ru"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-pdo": "*",
    "mongodb/mongodb": "^1.4",
    "robmorgan/phinx": "^0.10.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "MongoPhinx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MongoPhinx\\Tests\\": "tests/"
    }
  },
  "archive": {
    "exclude": [
      ".gitignore",
      "/tests"
    ]
  },
  "bin": [
    "bin/phinx-mongo"
  ],
  "support": {
    "email": "a.garipov@info74.ru"
  }
}
